                    Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using Microsoft.SqlServer.Dts.Runtime;
#endregion

[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
{
    public void Main()
    {
        // 1) RunId
        string runId = Guid.NewGuid().ToString();
        Dts.Variables["User::RunId"].Value = runId;

        // 2) ��������� ������
        int year = (int)Dts.Variables["User::vAcademicYear"].Value;
        int sem = (int)Dts.Variables["User::vSemester"].Value;

        // 3) SQL ��� DFT_07 FactStudentGroupSnapshot
        string sql07 =
@"SELECT
    CAST(GETDATE() AS date) AS SnapshotDate,
    s.StudentID,
    sg.GroupID,
    s.IsActive AS IsActiveStudent
FROM dbo.Student s
LEFT JOIN dbo.StudentGroup sg
  ON sg.StudentID = s.StudentID
 AND sg.DateTo IS NULL;";

        // 4) SQL ��� DFT_08 FactStudentSemesterPerformance
        string sql08 =
$@"SELECT
    MIN(CAST(g.GradeDate AS date)) AS PeriodStartDate,
    g.StudentID,
    g.AcademicYear,
    g.Semester,
    AVG(CAST(g.Points AS decimal(10,2)))     AS AvgPoints,
    AVG(CAST(g.StateGrade AS decimal(10,2))) AS AvgStateGrade,
    COUNT(DISTINCT ts.SubjectID)             AS SubjectsCount
FROM dbo.StudentGrade g
JOIN dbo.TeacherSubject ts
  ON ts.TeacherSubjectID = g.TeacherSubjectID
WHERE g.AcademicYear = {year}
  AND g.Semester     = {sem}
GROUP BY
    g.StudentID, g.AcademicYear, g.Semester;";

        Dts.Variables["User::SQL_DFT07"].Value = sql07;
        Dts.Variables["User::SQL_DFT08"].Value = sql08;

        // Success
        Dts.TaskResult = (int)DTSExecResult.Success;

    }
}]]></ProjectItem>
